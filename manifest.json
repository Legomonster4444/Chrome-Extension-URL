{
  "manifest_version": 2,
  "name": "URL Opener",
  "version": "1.0",
  "description": "Open any URL quickly.",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "output-onlinepngtools.png",
      "48": "output-onlinepngtools (1).png",
      "128": "output-onlinepngtools (2).png"
    }
  }
}
